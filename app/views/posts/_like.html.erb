<% if Like.find_by(user_digest: current_user.digest, post_id: post.id) %>
  <%= link_to("iine!!!",like_path(id: post.id), method: :delete, remote: true) %>
  <%= post.likes.count %>
<% else %>
  <%= link_to("iine?",likes_path(id: post.id), method: :post, remote: true) %>
  <%= post.likes.count %>
<% end %>
